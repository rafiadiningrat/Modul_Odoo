<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="bus_route_view_tree" model="ir.ui.view">
            <field name="name">bus.route.view.tree</field>
            <field name="model">bus.route</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name" required='1'></field>
                </tree>
            </field>
        </record>

        <record id="bus_route_action" model="ir.actions.act_window">
            <field name="name">Bus Route</field>
            <field name="res_model">bus.route</field>
            <field name="view_mode">tree</field>

            
        </record>
    
    </data>
    

</odoo>
